#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/cms/node_modules/.pnpm/plop@4.0.1/node_modules/plop/bin/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/cms/node_modules/.pnpm/plop@4.0.1/node_modules/plop/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/cms/node_modules/.pnpm/plop@4.0.1/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/cms/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/cms/node_modules/.pnpm/plop@4.0.1/node_modules/plop/bin/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/cms/node_modules/.pnpm/plop@4.0.1/node_modules/plop/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/cms/node_modules/.pnpm/plop@4.0.1/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/cms/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../plop@4.0.1/node_modules/plop/bin/plop.js" "$@"
else
  exec node  "$basedir/../../../../../../plop@4.0.1/node_modules/plop/bin/plop.js" "$@"
fi
