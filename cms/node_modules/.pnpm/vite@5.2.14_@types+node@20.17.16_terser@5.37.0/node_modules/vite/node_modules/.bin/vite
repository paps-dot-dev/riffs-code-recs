#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/riffs-code-recs-cms/node_modules/.pnpm/vite@5.2.14_@types+node@20.17.16_terser@5.37.0/node_modules/vite/bin/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/riffs-code-recs-cms/node_modules/.pnpm/vite@5.2.14_@types+node@20.17.16_terser@5.37.0/node_modules/vite/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/riffs-code-recs-cms/node_modules/.pnpm/vite@5.2.14_@types+node@20.17.16_terser@5.37.0/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/riffs-code-recs-cms/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/riffs-code-recs-cms/node_modules/.pnpm/vite@5.2.14_@types+node@20.17.16_terser@5.37.0/node_modules/vite/bin/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/riffs-code-recs-cms/node_modules/.pnpm/vite@5.2.14_@types+node@20.17.16_terser@5.37.0/node_modules/vite/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/riffs-code-recs-cms/node_modules/.pnpm/vite@5.2.14_@types+node@20.17.16_terser@5.37.0/node_modules:/Users/shawnpapineau/Developer/SPAP/riffs-code-recs/riffs-code-recs-cms/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
